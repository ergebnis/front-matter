{
  "name": "ergebnis/front-matter",
  "type": "library",
  "description": "Provides a GitHub repository template for a PHP library, using GitHub actions.",
  "homepage": "https://github.com/ergebnis/front-matter",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "symfony/yaml": "^5.3.6"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.16.0",
    "ergebnis/license": "^1.1.0",
    "ergebnis/php-cs-fixer-config": "~3.3.0",
    "ergebnis/test-util": "^1.5.0",
    "infection/infection": "~0.25.3",
    "phpunit/phpunit": "^9.5.10",
    "psalm/plugin-phpunit": "~0.16.1",
    "vimeo/psalm": "^4.13.1"
  },
  "config": {
    "platform": {
      "php": "7.4.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\FrontMatter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\FrontMatter\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/ergebnis/front-matter/issues",
    "source": "https://github.com/ergebnis/front-matter"
  }
}
