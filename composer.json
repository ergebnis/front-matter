{
  "name": "ergebnis/front-matter",
  "description": "Provides a front matter parser.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/front-matter",
  "support": {
    "issues": "https://github.com/ergebnis/front-matter/issues",
    "source": "https://github.com/ergebnis/front-matter"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "symfony/yaml": "^5.0.0 || ^6.0.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.28.3",
    "ergebnis/data-provider": "^1.2.0",
    "ergebnis/license": "^2.0.0",
    "ergebnis/php-cs-fixer-config": "~4.11.0",
    "fakerphp/faker": "^1.20.0",
    "infection/infection": "~0.26.6",
    "phpunit/phpunit": "^9.5.26",
    "psalm/plugin-phpunit": "~0.18.1",
    "vimeo/psalm": "^4.29.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\FrontMatter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\FrontMatter\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "7.4.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
